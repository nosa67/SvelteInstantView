{
	"name": "svelte-instant-view",
	"displayName": "Svelte Instant View",
	"description": "Show html file in the browser without transpile. But JavaScript does not work.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:svelte-instant-view.show"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "svelte-instant-view.show",
				"title": "Svelte instant viewer",
				"enablement": "filesExplorerFocus"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "svelte-instant-view.show",
					"when": "resourceExtname == .svelte"
				}
			]
		},
		"configuration": {
			"title": "Svelte Instant Viewer",
			"properties": {
				"svelte-instant-view.baseFile": {
					"type": "string",
					"default": "public/index.html",
					"description": "Base html file to insert svelte compornent."
				},
				"svelte-instant-view.insertTag": {
					"type": "string",
					"default": "contents",
					"description": "Element ID in base html to insert svelte template html."
				},
				"svelte-instant-view.browser": {
					"type": "string",
					"default": "chrome",
					"enum": [
						"chrome",
						"firefox",
						"edge"
					]
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.53.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.15.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	},
	"dependencies": {
		"iconv-lite": "^0.6.2",
		"jschardet": "^2.3.0",
		"jsdom": "^16.4.0",
		"sass": "^1.32.8"
	}
}
